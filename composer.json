{
    "name": "moontechs/openai-management",
    "description": "OpenAI files and batches management ",
    "keywords": [
        "laravel",
        "laravel-package",
        "filamentphp",
        "filamentphp-plugin",
        "openai-management",
        "openai",
        "openai-ui"
    ],
    "homepage": "https://github.com/moontechs/openai-management",
    "support": {
        "issues": "https://github.com/moontechs/openai-management/issues",
        "source": "https://github.com/moontechs/openai-management"
    },
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Moontechs",
            "email": "michael@kozii.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "openai-php/client": "^0.10.1",
        "spatie/laravel-package-tools": "^1.15.0",
        "symfony/http-client": "^7.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Moontechs\\OpenAIManagement\\": "src/",
            "Moontechs\\OpenAIManagement\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moontechs\\OpenAIManagement\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moontechs\\OpenAIManagement\\OpenAIManagementServiceProvider"
            ],
            "aliases": {
                "OpenAIManagement": "Moontechs\\OpenAIManagement\\Facades\\OpenAIManagement"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
